{
  "name": "vetrii-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "cross-env nest start --watch",
    "start:dev:env": "cross-env nodemon",
    "start:debug": "cross-env nest start --debug --watch",
    "start:prod": "cross-env node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "cross-env NODE_OPTIONS=\"--require=./ormconfig.cjs\" ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "schema:sync": "npm run typeorm -- schema:sync",
    "schema:log": "npm run typeorm -- schema:log",
    "schema:drop": "npm run typeorm -- schema:drop",
    "db:seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts",
    "db:seed:docker": "docker compose exec app pnpm db:seed",
    "migration:create": "node src/database/create-migration.js",
    "migration:run": "npm run typeorm -- migration:run -d dist/database/data-source.js",
    "migration:revert": "npm run typeorm -- migration:revert -d dist/database/data-source.js",
    "migration:show": "npm run typeorm -- migration:show -d dist/database/data-source.js",
    "migration:run:docker": "docker compose exec app pnpm migration:run",
    "migration:revert:docker": "docker compose exec app pnpm migration:revert",
    "migration:show:docker": "docker compose exec app pnpm migration:show"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.782.0",
    "@aws-sdk/client-s3": "^3.782.0",
    "@aws-sdk/client-sqs": "^3.821.0",
    "@bugsnag/js": "^8.4.0",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.2.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/throttler": "^6.0.0",
    "@nestjs/typeorm": "^10.0.1",
    "@ssut/nestjs-sqs": "^3.0.1",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "^10.0.0",
    "@xmldom/xmldom": "~0.8.11",
    "aws": "0.0.3-2",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.53.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "form-data": "^4.0.2",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.0.1",
    "mjml": "^4.15.3",
    "multer": "1.4.5-lts.2",
    "multer-s3": "^3.0.1",
    "mysql2": "^2.3.3",
    "nest-access-control": "^3.1.0",
    "nodemailer": "^7.0.3",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pinata": "^2.4.8",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.2",
    "source-map-support": "^0.5.21",
    "sqs-consumer": "^12.0.0",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.3.3",
    "uuid": "^10.0.0",
    "viem": "^2.26.2",
    "xpath": "^0.0.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/testing": "^10.2.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mjml": "^4.7.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.19.14",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/supertest": "^2.0.16",
    "@types/xmldom": "^0.1.34",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsc-watch": "^6.0.4"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pnpm": {
    "overrides": {
      "mysql2@<3.9.4": ">=3.9.4",
      "mysql2@<3.9.7": ">=3.9.7",
      "mysql2@<3.9.8": ">=3.9.8",
      "mysql2@<3.9.3": ">=3.9.3",
      "tmp@<=0.2.3": ">=0.2.4",
      "html-minifier": "npm:html-minifier-terser@^7.2.0"
    }
  }
}
